{
  "name": "learn-network",
  "version": "1.0.0",
  "description": "network ====",
  "scripts": {
    "start": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "pretls:handshake": "./examples/tsl/handshake/cert.sh &  sleep 2",
    "tls:handshake": "lsof -ti:8443 | xargs kill -9 2>/dev/null || true && node examples/tsl/handshake/server.js & sleep 2 && node examples/tsl/handshake/client.js",
    "tls:handshake:v1.2": "lsof -ti:8443 | xargs kill -9 2>/dev/null || true && export TLS_VERSION=TLSv1.2 && node examples/tsl/handshake/server.js & sleep 2 && node examples/tsl/handshake/client.js",
    "websocket:handshake": "lsof -ti:8080 | xargs kill -9 2>/dev/null || true && node examples/websocket/handshake/server.js & sleep 2 && open examples/websocket/handshake/client.html",
    "xmlhttprequest:withCredentials": "node examples/browser-api/XMLHttpRequest/withCredentials/server.js",
    "xmlhttprequest:responseType": "node examples/browser-api/XMLHttpRequest/responseType/server.js",
    "xmlhttprequest:download": "node examples/browser-api/XMLHttpRequest/download/server.js",
    "xmlhttprequest:upload": "node examples/browser-api/XMLHttpRequest/upload/server.js",
    "fetch:withCredentials": "node examples/browser-api/fetch/withCredentials/server.js",
    "fetch:abort": "node examples/browser-api/fetch/abort/server.js",
    "fetch:responseType": "node examples/browser-api/fetch/responseType/server.js",
    "fetch:upload": "node examples/browser-api/fetch/upload/server.js",
    "fetch:download": "node examples/browser-api/fetch/download/server.js",
    "sendBeacon": "node examples/browser-api/sendBeacon/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenHeart/learn-network.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zenHeart/learn-network/issues"
  },
  "homepage": "https://github.com/zenHeart/learn-network#readme",
  "devDependencies": {
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "mermaid": "^11.6.0",
    "vitepress": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.17"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "koa": "^2.16.1",
    "koa-send": "^5.0.1",
    "koa-static-cache": "^5.1.4",
    "markdown-it-checkbox": "^1.1.0",
    "on-headers": "^1.0.2"
  }
}